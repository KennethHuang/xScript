<?xml version="1.0"?>
<script>
	<!--Tag <set> define in <script>, the variable is public default.-->
	<set var="final name" value="{$author}" default="Kenneth Huang"/>
	<method name="main">
		<set var="author" value="{$name} Huang">
			<condition cond="#contains({$name},Huang)" value="Kenny Huang"/>
		</set>
		<println>Author: {$author}</println>
	</method>
	<method name="getAuthor">
		<return value="Kenny"/>
	</method>
	<method name="getHost">
		<return value="GitHub"/>
	</method>
</script>
